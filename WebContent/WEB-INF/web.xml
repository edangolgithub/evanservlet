<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:web="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">
  <servlet>
    <servlet-name>loginServlet</servlet-name>
    <servlet-class>evan.servlet.OnServletLogin</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>configservlet</servlet-name>
    <servlet-class>evan.servlet.Servletconfig</servlet-class>
    <init-param>
      <param-name>driver</param-name>
      <param-value>sun.jdbc.odbc.JdbcOdbcDriver</param-value>
    </init-param>
  </servlet>
  <servlet>
    <servlet-name>FirstServlet</servlet-name>
    <servlet-class>evan.servlet.First</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>servlet1</servlet-name>
    <servlet-class>evan.servlet.servlet1</servlet-class>
  </servlet>
  <servlet>
  <servlet-name>egs</servlet-name>
  <servlet-class>evan.servlet.EvanGenericServlet</servlet-class>
  </servlet>
  <servlet-mapping>
  <servlet-name>egs</servlet-name>
  <url-pattern>/egs</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>loginServlet</servlet-name>
    <url-pattern>/login</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>FirstServlet</servlet-name>
    <url-pattern>/first</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>configservlet</servlet-name>
    <url-pattern>/Servletconfig</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>servlet1</servlet-name>
    <url-pattern>/servlet1</url-pattern>
  </servlet-mapping>
  <welcome-file-list>
    <welcome-file>e.html</welcome-file>
    <welcome-file>index.html</welcome-file>
  </welcome-file-list>
</web-app>